   "license": "ISC",
   "devDependencies": {
      "@parcel/transformer-sass": "^2.8.2",
      "parcel": "^2.8.2",
      "postcss": "^8.4.20",
      "postcss-jit-props": "^1.0.8",
      "postcss-preset-env": "^7.8.3",
      "sass": "^1.57.1"
   },
   "dependencies": {
      "open-props": "^1.5.3"
   }
}
