{
   "name": "18_notification_page_2",
   "version": "1.0.0",
   "description": "notification 2 with parcel",
   "source": "index.html",
   "scripts": {
      "clean-dist": "rm -rf dist && mkdir dist",
      "copy-img": "cp -vR ./src/images/* dist",
      "copy-fonts": "cp -r src/fonts/* dist",
      "init": "npm run clean-dist && npm run copy-img",
      "start": "npm run init && parcel",
      "build": "npm run init && parcel build"
   },
